<div class="row pt-4">
  <div class="card glass">
    <div class="card-body">
      <div class="row">
        <div class="col-sm-3">
          <p class="mb-0"> Nombre completo</p>
        </div>
        <div class="col-sm-9">
          <p class="mb-0">{{userInfo.first_name}}
            {{userInfo.last_name}}</p>
        </div>
      </div>
      <hr />
      <div class="row">
        <div class="col-sm-3">
          <p class="mb-0">Email</p>
        </div>
        <div class="col-sm-9">
          <p class="mb-0">{{userInfo.email}}</p>
        </div>
      </div>
      <hr />
      <div class="row">
        <div class="col-sm-3">
          <p class="mb-0">Teléfono</p>
        </div>
        <div class="col-sm-9">
          <p class="mb-0">{{userInfo.phone}}</p>
        </div>
      </div>
      <hr />
      <div class="row">
        <div class="col-sm-3">
          <p class="mb-0">Dirección</p>
        </div>
        <div class="col-sm-9">
          <p class="mb-0">{{userInfo.address}}</p>
        </div>
      </div>
    </div>
  </div>
</div>